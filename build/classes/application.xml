<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="application" extends="struts-default">
		<action name="addStudentDetails" class="com.struts.action.Application" method="addStudentDetails">
			<result name="success">/application/studentExperience.jsp</result>
			<result name="error">/application/error.jsp</result>
			<result name="input">/application/addStudent.jsp</result>
		</action>
		<action name="addExperienceDetails" class="com.struts.action.Application2" method="addExperienceDetails">
			<result name="success">/application/success.jsp</result>
			<result name="error">/application/error.jsp</result>
		</action>
	</package>
</struts>